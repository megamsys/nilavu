var selected_entity = '#product_<%= @apps_item.product_id %>';

$(selected_entity).toggle();

$(selected_entity).fadeIn("slow");

var to_insert_content = '<%= escape_javascript(render :partial => "apps_items/remove_button", :locals => {:apps_item_el => @apps_item, :product => @product}) %>';
	
insertAt(selected_entity,to_insert_content);

