<%= render 'layouts/header' %>
<div class="container main-cover">
  <div class="rt-box col-xs-12 col-sm-8 col-md-9 col-lg-9 col-xl-9 c_bg_white c_height_5">
    <div class="row c_pading-b15">
      <div class="margin_15">
        <div class="col-xs-9 col-sm-6 col-md-7">
          <h3> We flunked. </h3>
        </div>
      </div>
    </div>
    <div class="margin_30 pad_t25">
      <div class="bottom-divider"></div>
      <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
        <div class="row app_box">
          <%= link_to "http://bit.ly/megamchat", { :target => '_blank'} do %>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-3 app_cover">
              <div class="app_inner">
                <div class="app_new">
                  <span class="glyphicon icon_chat"></span>
                  <p>
                    Chat now
                </p>
                </div>
              </div>
            </div>
          <% end %>
          <%= link_to "http://bit.ly/megamchat", { :target => '_blank'} do %>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-3 app_cover">
              <div class="app_inner">
                <div class="app_new">
                  <span class="glyphicon icon_book"></span>
                  <p>Documentation</p>
                </div>
              </div>
            </div>
          <% end %>
          <%= link_to "http://support.megam.io", { :target => '_blank'} do %>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-3 app_cover">
              <div class="app_inner">
                <div class="app_new">
                  <span class="glyphicon icon-phone"></span>
                  <p>US   1-800-618-6813</p>
                  <p>Intl 91-98405-59300
                </div>
              </div>
            </div>
          <% end %>
          <%= link_to "https://github.com/megamsys", { :target => '_blank'} do %>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-3 app_cover">
              <div class="app_inner">
                <div class="app_new">
                  <span class="glyphicon icon-github"></span>
                  <p>megamsys</p>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
<%= render 'layouts/footer' %>
