<% if @err_msg != nil %>
var selected_entity = '#appboilers_postfinish_popup_error';
$("#err_msg").html('<%= @err_msg %>');
$(selected_entity).modal({backdrop : false,keyboard: false,show : true});
<% else %>
var selected_entity = '#appboilers_postfinish_popup';
$("#res_msg").html('<%= @res_msg %>');
$(selected_entity).modal({backdrop : false,keyboard: false,show : true});
<% end %>
