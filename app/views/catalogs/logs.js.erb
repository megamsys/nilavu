
var rootElement = angular.element(document);
    rootElement.ready(function(){
        rootElement.injector().invoke(function($compile){
          var $content = $('#content'),
              scope =  $content.scope();
          $content.html("<%= escape_javascript(render :partial => "catalogs/logs") %>");
          $compile($content.contents())(scope);
          scope.$digest();
        });
      });
