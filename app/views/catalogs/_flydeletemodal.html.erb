<!-- APP INFO MODAL -->
<div class="modal fade" id="megam_flydeletemodal" tabindex="-1" role="basic" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content c_modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
				<h4 class="modal-title"><%= @dmsg[:title].camelize %></h4>
			</div>
			<div class="modal-body text-center">
				 <%= @dmsg[:message] %>
			</div>
			<div class="modal-footer c_modal-footer">
				<%= link_to  "Ok", "#{@dmsg[:redirect]}", :class => "btn btn-success a_pad", :target => "_self" %>
			</div>
		</div>
		<!-- /.modal-content -->
	</div>
	<!-- /.modal-dialog -->
</div>