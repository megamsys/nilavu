$('#cloudbook_clone_popup').modal('hide');
<% if @res_msg %>
var selected_entity = '#clone_postfinish_popup_error';
$("#err_msg").html('<%= @res_msg %>');
$(selected_entity).modal({backdrop : false,keyboard: false,show : true,remote : '#'});

<% else %>
var selected_entity = '#clone_postfinish_popup';
$(selected_entity).modal({backdrop : false,keyboard: false,show : true,remote : '#'});
<% end %>
