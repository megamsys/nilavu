<!-- BEGIN HEADER -->
<div class="header navbar navbar-fixed-top">
	<div class="container">
		<!-- BEGIN TOP NAVIGATION BAR -->
		<div class="header-inner">
			<div class="row">
				<div class="col-lg-2 col-md-2 col-sm-3 col-xs-5">
					<!-- BEGIN LOGO -->
					<a class="navbar-brand" href="http://www.gomegam.com"> <%= image_tag "dash-logo.png", :alt => "logo", :class => "img-responsive" %> </a>
					<!-- END LOGO -->
				</div>

				<div class="col-lg-3 col-md-3 col-sm-3 col-xs-6 col-lg-offset-7 col-md-offset-7 col-sm-offset-6">
					<div class="row">

						<!-- <div class="col-xs-4 col-sm-1"> -->
						<ul class="nav navbar-nav pull-right">
							<!-- BEGIN NOTIFICATION DROPDOWN -->
							<li class="dropdown" id="header_notification_bar">
								<a href="#" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown"
								data-close-others="true"> <i class="c_glob glyphicon glyphicon-user"></i> <!-- <span class="badge badge-success">6</span> --> </a>
								<ul class="dropdown-menu extended notification">
									<li>
										<ul class="dropdown-menu-list scroller">
											<li>
												<%= link_to edit_user_path(current_user) do %>
												<span class="label label-sm label-icon label-success"><i class="glyphicon glyphicon-user"></i></span>
												My Profile
												<% end %>
											</li>
											<li>
												<%= link_to signout_path, method: "delete" do %>
												<span class="label label-sm label-icon label-danger"><i class="glyphicon glyphicon-off"></i></span>
												Sign Out
												<% end %>
											</li>
										</ul>
									</li>
								</ul>
							</li>
							<!-- END NOTIFICATION DROPDOWN -->
						</ul>
						<!-- </div>
						<div class="col-xs-4 col-sm-1">	 -->
						<ul class="nav navbar-nav pull-right m10">
							<!-- BEGIN NOTIFICATION DROPDOWN -->
							<li class="dropdown" id="header_notification_bar">
								<a href="#" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown"
								data-close-others="true"> <i class="c_glob glyphicon glyphicon-globe"></i> <span class="badge badge-success" id="notification"> <i class="icon-bell"></i></span> </a>
								<ul class="dropdown-menu extended notification">
									<li>
										<p>
											You have new notifications
										</p>
									</li>
									<li>
										<ul class="dropdown-menu-list scroller", id="events">
											
										</ul>
									</li>
									<!--<li class="external">
									<a href="#">See all notifications <i class="m-icon-swapright"></i></a>
									</li>-->
								</ul>
							</li>
							<!-- END NOTIFICATION DROPDOWN -->
						</ul>
						<!-- </div>
						<div class="col-xs-4 col-sm-1"> -->
