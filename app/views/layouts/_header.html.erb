						<a href="javascript:;" class="navbar-toggle collapsed pull-right" data-toggle="collapse" data-target=".navbar-collapse"> <%= image_tag "menu-toggler.png", :alt => "" %></a>
						<!-- END RESPONSIVE MENU TOGGLER -->
						<!-- </div> -->
					</div>
				</div>

			</div>
		</div>
		<!-- END TOP NAVIGATION BAR -->
	</div>
</div>
