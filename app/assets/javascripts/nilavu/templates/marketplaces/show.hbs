<div class="rt-box col-xs-12 col-sm-9 col-md-10 c_bg_white c_height_5">
    {{topic-list-header currentUser=currentUser title=title nameEnabled=nameEnabled name=name}}
    <div class="bottom-divider"></div>
    <div class="cat_title">
        {{#each orderedCatTypes as |member|}}
        <h4>{{member.cattype}}<small>
                <i>{{member.id}}</i>
            </small>
        </h4>
        <div class="col-xs-12">
            <div class="row app_box ">
                {{marketplace-listings member=member results=model.results action="createTopic"}}
            </div>
        </div>
        {{/each}}
    </div>
</div>
